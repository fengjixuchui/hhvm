#
# Test arrays for hhbc asm.
#

.adata my_array = """Y:2:{s:3:"foo";s:3:"bar";s:3:"baz";s:4:"blah";}""";

.class ClassWithArray {
  .property [static public] arr =
            """Y:4:{i:0;i:1;i:1;i:2;i:2;i:3;i:3;i:4;}""";
}

.function ArrayMember() {
  NullUninit
  NullUninit
    String "arr"
    String "ClassWithArray"
    ClassGetC
    CGetS Mutable
  FCallFuncD <> 1 1 "" - "" "var_dump"
  PopC

  String "ClassWithArray"
  SetL $x
  PopC
  NullUninit
  NullUninit
    String "arr"
    Int 2
    CGetL $x
    ClassGetC
    BaseSC 2 0 Warn Mutable
    QueryM 3 CGet EC:1 Mutable
  FCallFuncD <> 1 1 "" - "" "var_dump"
  PopC

  True
  RetC
}

.function ["__EntryPoint"("""y:0:{}""")] main() {
  NullUninit
  NullUninit
  FCallFuncD <> 0 1 "" - "" "ArrayMember"
  PopC

  Array @my_array
  SetL $arr
  PopC

  BaseL $arr Warn
  QueryM 0 CGet ET:"foo" Mutable
  String "\n"
  Concat
  Print
  PopC

  String "one"
  BaseL $arr Define
  SetM 0 EI:1 Mutable
  PopC

  BaseL $arr Warn
  QueryM 0 CGet EI:1 Mutable
  String "\n"
  Concat
  Print
  PopC

  String "baz"
  SetL $key
  PopC
  BaseL $arr Unset
  UnsetM 0 EL:$key Mutable

  # Test that it's unset
  CGetL $arr
  BaseC 0 Warn
  QueryM 1 Isset EL:$key Mutable
  JmpZ unset_label

  String "This should be impossible...\n"
  Print
  PopC

  Null
  RetC

unset_label:  String "win\n"
              Print
              PopC
              NullUninit
              NullUninit
              CGetL $arr
              FCallFuncD <> 1 1 "" - "" "var_dump"
              PopC
              NullUninit
              NullUninit
              Int 1
              String "b"
              NewVArray 2
              FCallFuncD <> 1 1 "" - "" "var_dump"
              PopC

              Null
              RetC
}
